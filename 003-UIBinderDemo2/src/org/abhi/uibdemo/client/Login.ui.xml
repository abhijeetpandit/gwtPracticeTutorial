<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:res="urn:with:com.abhi.uibdemo.client.LoginResources">
	<ui:with type="org.abhi.uibdemo.client.LoginResources" field="res"></ui:with>
	<g:HTMLPanel>
		<div align="center">
			<g:VerticalPanel styleName="{res.css.background}">
				<g:Label text="Login" styleName="{res.css.blackText}"/>
				<g:TextBox ui:field="loginBox" styleName="{res.css.box}"/>
				<g:Label text="Password" styleName="{res.css.blackText}"/>
				<g:PasswordTextBox ui:field="passwordBox" styleName="{res.css.box}"/>
				<g:HorizontalPanel verticalAlignment="middle">
					<g:Button ui:field="buttonSubmit" text="Login" styleName="{res.css.loginButton}"/>
					<g:CheckBox ui:field="myCheckBox"/>
					<g:Label ui:field="myLabel" text="Remember Me" styleName="{res.css.blackText}"/>
				</g:HorizontalPanel>
				<g:Label ui:field="completionLabel1" styleName="{res.css.redText}"/>
				<g:Label ui:field="completionLabel2" styleName="{res.css.redText}"/>
			</g:VerticalPanel>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 